{
  "name": "voice-text-app",
  "description": "An app that convert speech to text",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "jest": "^29.2.1"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0"
  }
}
